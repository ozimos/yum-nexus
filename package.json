  "scripts": {
    "format": "npx prettier --write './**/*.{js,ts,md}'",
    "dev": "NODE_OPTIONS='--inspect' NODE_DEBUG=app next",
    "build": "nexus build && next build",
    "start": "next start",
    "nexus:reflection": "nexus dev --reflection --entrypoint pages/api/graphql.ts",
    "nexus:check": "nexus build --entrypoint pages/api/graphql.ts",
    "codegen": "graphql-codegen --config codegen.yml",
    "generate": "prisma generate",
    "postmigrate": "dotenv -e prisma/.env -- node prisma/migrate",
    "migrate": "prisma migrate up --experimental",
    "migrate:save": "prisma migrate save --experimental",
    "seed": "dotenv -e prisma/.env  -- node prisma/seed",
    "drop": "dotenv -e prisma/.env  -- node prisma/drop",
    "reset": "yarn drop && yarn migrate",
    "test": "dotenv -e prisma/.env -e .env -e .env.test -- jest",
    "deploy:preview": "vercel",
    "deploy:prod": "vercel --prod"
  },
